{
  "name": "dslink-java-dummy",
  "version": "0.1.0",
  "description": "Dummy DSLink",
  "license": "Apache",
  "author": {
    "name": "Joel Hebert",
    "email": "j.hebert@dglogik.com"
  },
  "main": "bin/dslink-java-dummy",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-dummy"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-dummy/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "Dummy"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.dsa.iot.dummy.Main"
    }
  }
}
